                           #Page-4 Langselect Screen



import React, { Component } from 'react';
import { Text, View, ScrollView, StyleSheet, Image, FlatList, TouchableOpacity } from 'react-native';
import RadioForm, { RadioButton, RadioButtonInput, RadioButtonLabel } from 'react-native-simple-radio-button';
import { Divider, Input } from 'react-native-elements';
// import RadioButtonRN from 'radio-buttons-react-native';

// import { SafeAreaView } from 'react-native-safe-area-context';

export default class LangSelectScreen extends Component {
    constructor() {
        super();
        this.state = {
            langOptions: [
                { label: "English", value: 0 },
                { label: "Hindi (हिंदी)", value: 1 },
                { label: "Tamil (தமிழ்)", value: 2 },
                { label: "Telugu (తెలుగు)", value: 3 },
                { label: "Malyalam (മല്യാലം)", value: 4 },
                { label: "Marathi (मराठी)", value: 5 },
                { label: "Kannada (ಕನ್ನಡ)", value: 6 }
            ],
            selectedLang :0
        }
    }
    selectedLanguage = () => {
        console.log(this.state.selectedLang)
        if(this.state.selectedLang=="0")
        {
            this.props.navigation.navigate('googleAuthentication')
        }
    }
    render() {
        return (
            <View style={styles.container}>
            <ScrollView >

                <View style={[styles.imageViewStyle]}>
                    <Image source={require('../SelLangImages/SelectLangImage.png')} style={styles.imageStyle} />
                </View >
                <View style={styles.radFormView}>
                    <RadioForm
                        radio_props={this.state.langOptions}
                        initial={0}
                        buttonSize={12}
                        buttonOuterSize={25}
                        radioStyle={{paddingBottom:20,marginLeft:'4%'}}
                        selectedButtonColor={'#F84464'}
                        buttonColor={'silver'}
                        labelStyle={{ fontSize: 17 }}
                        onPress={(val)=>{this.setState({selectedLang:val})}} />
                       
                    {/* <Divider style={{ backgroundColor: 'blue' }} />; */}
                 </View>
            </ScrollView>
             <View style={styles.touchOpStyle}>
                   <TouchableOpacity style={styles.buttonStyle}
                   onPress={ this.selectedLanguage.bind(this)}>
                       <Text style={styles.btnText}>LET'S GET STARTED</Text>
                   </TouchableOpacity>
             </View>
            </View>
        );
    }
}
const styles = StyleSheet.create({
    container: {
        flex: 1,
        backgroundColor: '#ffffff',
    },
    imageViewStyle: {
        alignItems: 'center',
        height:250
        // backgroundColor: 'red',
    },
    imageStyle: { 
        resizeMode: 'cover',
    },
    radFormView: {
      marginVertical:20,    
    },
    touchOpStyle:{
        width:'100%',
        bottom:0
      },
     btnText: {
        textAlign: 'center',
        fontSize: 18,
        color: 'white'
      },
      buttonStyle: {
        backgroundColor: '#F84464',
        padding: 10,
      }
})





