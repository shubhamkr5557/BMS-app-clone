                      #Page-3 Home Screen



import React, { Component } from 'react';
import { Text, View, ScrollView, StyleSheet, Image, TouchableOpacity ,Dimensions} from 'react-native';
import IconSearch from 'react-native-vector-icons/EvilIcons';
import IconScan from 'react-native-vector-icons/MaterialCommunityIcons';
import IconNotification from 'react-native-vector-icons/Ionicons';
import IconLeft from 'react-native-vector-icons/AntDesign';

// import IconUser from 'react-native-vector-icons/FontAwesome5';
import Orientation from 'react-native-orientation';

 const images = [require('../HomeScreenImages/SadhguruLive.jpg'),
                 require('../HomeScreenImages/NCPA-City.jpg'),
                 require('../HomeScreenImages/SummerExpress.jpg')];
                
 const imagesRecMOvie = [{label:require('../HomeScreenImages/Kurian_Jawan.jpg'), value:'Kuriyan Jawan Bapu Pareshaan'},
                         {label:require('../HomeScreenImages/Godzilla.jpg'),value:'Godzilla vs. Kong'},
                         {label:require('../HomeScreenImages/Mumbai_Saga.jpg'),value:'Mumbai Saga'},
                         {label:require('../HomeScreenImages/99Songs.jpg'),value: '99 Songs'},
                         { label:require('../HomeScreenImages/Roohi.jpg'),value:'Roohi'}
                        ];  
 const imagesBestOfEnter1= [
    require('../HomeScreenImages/workshops-collection.png'), 
    require('../HomeScreenImages/music-shows-collection.png'), 
    require('../HomeScreenImages/interactive-games-collection.png'), 
    require('../HomeScreenImages/esports-collection.png'),
    require('../HomeScreenImages/cooking-collection.png'), 
    ] ;  

 const imagesBestOfEnter2=[
    require('../HomeScreenImages/theatre-shows-collection.png'),
    require('../HomeScreenImages/self-improvement-collection.png'),
    require('../HomeScreenImages/kids-collection.png'),
    require('../HomeScreenImages/fitness-collection.png'),
    require('../HomeScreenImages/dance-collection.png')
 ];

let device_width=Dimensions.get("window").width;
let screenDecide = {

    height: Dimensions.get("window").height,
    width: Dimensions.get("window").width,  
};
export default class HomeScreen extends Component {
    scrollRef=React.createRef()
    constructor(props){
        super(props);
        this.state = {
          selectedIndex:0,
          isLandscape:false
        }
    }

    setSelectedIndex= event => {
        const viewSize= event.nativeEvent.layoutMeasurement.width;
        const contentOffset= event.nativeEvent.contentOffset.x;
        const selectedIndex=Math.floor(contentOffset/viewSize);
        this.setState({selectedIndex})
    }

    componentDidMount(){
        // To add listener
        Orientation.addOrientationListener(this._orientationDidChange);
        // Orientation.lockToPortrait();
        setInterval(()=>{
          this.setState((prev) => ({selectedIndex:
            prev.selectedIndex===images.length-1 
            ? 0
            : prev.selectedIndex+1}),
           () => {
               this.scrollRef.current.scrollTo({
                   animated:true,
                   y:0,
                   x:device_width*this.state.selectedIndex
               })
           }
          )
        },3000)
    }
    _orientationDidChange = (orientation) => {
        const { height, width } = Dimensions.get('window');
       
  const min = Math.min(height, width);
  const max = Math.max(height, width);
  const isLandscape = orientation === 'LANDSCAPE';
  screenDecide = {
    height: isLandscape ? min : max,
    width: isLandscape ? max : min,
  }
//   console.log(screenDecide.height,screenDecide.width)

  this.setState({isLandscape})
    }


    renderTopMenu()
    {
        return(  <View style={styles.slideHeaderIconMainView}>  
            <View> 
            <TouchableOpacity onPress={() => {this.props.navigation.navigate('movies')}}>    
             <Image source={require('../HomeScreenIcons/Movies-Icon.png')} style={{height:80,width:80}}/>
             </TouchableOpacity>  
            </View> 
            <View> 
            <TouchableOpacity>          
             <Image source={require('../HomeScreenIcons/Stream-Icon.png')}  style={{height:80,width:80}}/>
             </TouchableOpacity>  
            </View> 
            <View>
            <TouchableOpacity  onPress={() => this.props.navigation.navigate('Events')}>           
             <Image source={require('../HomeScreenIcons/Events-Icon.png')}  style={{height:80,width:80}}/>
             </TouchableOpacity>  
            </View> 
            <View> 
            <TouchableOpacity>          
             <Image source={require('../HomeScreenIcons/Sports-Icon.png')}  style={{height:80,width:80}}/>
             </TouchableOpacity>  
            </View> 
            <View>
            <TouchableOpacity onPress={() => {this.props.navigation.navigate('plays')}}>           
             <Image source={require('../HomeScreenIcons/Plays-Icon.png')}  style={{height:80,width:80}}/>
             </TouchableOpacity>  
            </View> 
            <View> 
            <TouchableOpacity  onPress={() => {this.props.navigation.navigate('activities')}}>          
             <Image source={require('../HomeScreenIcons/Activities-Icon.png')}  style={{height:80,width:80}}/>
            </TouchableOpacity>
             </View> 
         </View> )
    }

    componentWillUnmount() {
        Orientation.getOrientation((err, orientation) => {
          console.log(`Current Device Orientation: ${orientation}`);
        });
     
     
        // To remove listener
        Orientation.removeOrientationListener(this._orientationDidChange);
      }

    render() {
        const {selectedIndex,isLandscape}=this.state
        return (
            <View style={styles.container}>
                <View style={styles.headerTextView}>
                    <View style={{ flex: 2 }}>
                        <Text style={styles.headerText}>It All Starts Here</Text>
                        <TouchableOpacity>
                            <Text style={styles.headerTouchOpTxt}>Ludhiana <IconLeft name="right" size={10} color='white'  /></Text>
                          
                        </TouchableOpacity>
                    </View>

                    <View style={{ flex: 1 }}>
                        <View style={styles.headerIconView}>
                            <TouchableOpacity><IconScan name="qrcode-scan" size={25} color='white' style={{ paddingRight: 20 }} /></TouchableOpacity>
                            <TouchableOpacity onPress={() => {this.props.navigation.navigate('notification')}}>
                                <IconNotification name="notifications-outline" size={25} color='white' style={{ paddingRight: 20 }} /></TouchableOpacity>
                            <TouchableOpacity><IconSearch name="search" size={32} color='white' style={{ paddingRight: 20 }} /></TouchableOpacity>
                        </View>
                    </View>
                </View>
                <ScrollView >
                    {!isLandscape?
                    <ScrollView horizontal={true} showsHorizontalScrollIndicator={false} style={[styles.slideHeaderIconScroll]} >
                    {this.renderTopMenu()}
                     </ScrollView>
                     : <View style={[styles.slideHeaderIconScroll1]}>{this.renderTopMenu()}</View>          
           }
                    <View style={[styles.autoScrollTop,{width:screenDecide.width}]}>          
                       <ScrollView horizontal pagingEnabled 
                       onMomentumScrollEnd={this.setSelectedIndex} 
                       showsHorizontalScrollIndicator={false}
                       ref={this.scrollRef}>
                          {images.map((item) =>
                             { return (
                                <Image style={[styles.autoScrollTop,{resizeMode:'stretch'}]}
                                source={item}
                                /> )
                            }
                            )}
                       </ScrollView> 
                       <View style={styles.circleView}>
                           {
                               images.map((item,i) =>
                               { return(
                                  <View key={i} style={[styles.whiteCircle,{opacity: i===selectedIndex?1:0.5}]}/>
                               )}
                               )
                           }
                       </View>
                     </View>
                     <View style={[styles.recMovTextHeadView]}>
                         <Text style={styles.recommendMovTxtHead}> Recommended Movies</Text>
                     </View>
                   <ScrollView horizontal={true} showsHorizontalScrollIndicator={false}>
                        <View style={styles.recMovView}>
                          {imagesRecMOvie.map((item) =>
                             { return (
                                <TouchableOpacity>
                                  
                                    <Image style={styles.recMovImg}  source={item.label} /> 
                                 
                                 <View style={{paddingLeft:10,width:150,flexWrap:'wrap'}}>
                                   <Text style={styles.recommendMovTxt}>{item.value}</Text>
                                 </View>
                                 </TouchableOpacity>)
                            }
                            )}
                        </View>
                   </ScrollView>
                   <View style={styles.recMovTextHeadView}>
                       <View style={{marginBottom:5}}>
                         <Text style={styles.recommendMovTxtHead}> The Best Of Entertainment</Text>
                       </View>
                       <View>
                         <Text style={[styles.bestOfEnterTxtHeadSmall]}> Step out or stay in, interesting experiences for everyone.</Text>
                       </View>
                    </View>
                    <ScrollView horizontal={true} showsHorizontalScrollIndicator={false}>
                        <View>
                        <View style={[styles.bestOfEnterImagesView,{marginBottom:10}]}>
                          {imagesBestOfEnter1.map((item) =>
                             { return (
                                <TouchableOpacity>
                                <Image style={[styles.bestOfEnterImages]}
                                source={item}
                                /> 
                               </TouchableOpacity>)
                            }
                            )}
                        </View>
                        <View style={styles.bestOfEnterImagesView}>
                          {imagesBestOfEnter2.map((item) =>
                             { return (
                                <TouchableOpacity>
                                <Image style={[styles.bestOfEnterImages]}
                                source={item}
                                /> 
                                </TouchableOpacity>)
                            }
                            )}
                        </View>
                        </View>
                   </ScrollView>
               </ScrollView>
               <View style={styles.footerTextView}>
                   <TouchableOpacity>
                    <View style={{alignItems:'center'}}>
                       <Image  source={require('../HomeScreenImages/BMS-Logo1.png')}  style={[{height:30,width:30}]} />
                       <Text style={{fontSize:15,color:'rgb(102, 102, 102)',paddingTop:0}}>Home</Text>
                    </View>
                   </TouchableOpacity>
                   <TouchableOpacity onPress={() => {this.props.navigation.navigate('fanhood')}}>
                    <View style={{alignItems:'center'}}>
                       <Image  source={require('../HomeScreenImages/StoreIcon.png')} tintColor='dimgrey' style={{height:30,width:30}} />
                       <Text style={{fontSize:15,color:'rgb(102, 102, 102)',paddingTop:0}}>Store</Text>
                    </View>
                   </TouchableOpacity>
                   <TouchableOpacity>
                    <View style={{alignItems:'center'}}>
                       <Image  source={require('../HomeScreenImages/megaphone.png')} tintColor='dimgrey' style={{height:30,width:30}} />
                       <Text style={{fontSize:15,color:'rgb(102, 102, 102)'}}>Buzz</Text>
                    </View>
                   </TouchableOpacity>
                   <TouchableOpacity onPress={() => {this.props.navigation.navigate('profile')}}>
                    <View style={{alignItems:'center'}}>
                    <Image  source={require('../HomeScreenImages/user.png')} tintColor='dimgrey' style={{height:30,width:30}} />
                       {/* <IconUser name="user-alt" size={25} color='dimgrey'/> */}
                       <Text style={{fontSize:15,color:'rgb(102, 102, 102)'}}>Profile</Text>
                    </View>
                   </TouchableOpacity>
                  
               </View>
            </View>
        );
    }
}

const styles = StyleSheet.create({
    container: {
        flex: 1,
